	<!-- 
=========================== ORGANISATION TYPES ===============================
-->
	<xsd:complexType name="organisationTypeSupp">
		<xsd:sequence>
			<xsd:element name="hpioNumber" type="hpioNumber"/>
			<xsd:element name="name" type="xsd:string" maxOccurs="unbounded"/>
			<xsd:element name="organisationService" type="organisationServiceType" maxOccurs="unbounded"/>
			<xsd:element name="participatingOrganisation" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="hpioNumber" type="hpioNumber"/>
						<xsd:element name="name" type="xsd:string" maxOccurs="unbounded"/>
						<xsd:element name="organisationService" type="organisationServiceType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
